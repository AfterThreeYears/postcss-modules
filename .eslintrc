{
  "extends": "airbnb-base",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "node": true,
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "template-curly-spacing": [2, "always"],
    "max-len": [2, 80, 4],
    "key-spacing": 0,
    "no-multi-spaces": 0,
  }
}
